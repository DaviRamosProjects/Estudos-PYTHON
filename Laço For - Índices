estoque = [1200, 300, 800, 1500, 2750, 400, 20, 23, 79, 111, 34, 67]
produtos = ['coca', 'pepsi', 'guarana', 'laranja', 'romã', 'fanta', 'alho',
            'veja', 'alvejante', 'rodo', 'panelas', 'alface']
nivel_minimo = 50

for i, qtd in enumerate(estoque):
    if qtd < nivel_minimo:
        print('A quantidade de {} é insuficiente {}.'.format(produtos[i], qtd))
